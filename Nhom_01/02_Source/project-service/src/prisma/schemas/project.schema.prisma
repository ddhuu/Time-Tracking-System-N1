model Project {
    id             Int       @id @default(autoincrement())
    name           String
    color          String?
    project_number Int
    order_number   Int?
    order_date     DateTime?
    start_date     DateTime?
    end_date       DateTime?
    budget         Float?

    teams     Team[]     @relation("ProjectTeam")
    customers Customer[] @relation("CustomerProject")

    created_at DateTime  @default(now())
    updated_at DateTime  @updatedAt
    deleted_at DateTime?

    @@map("projects")
}
